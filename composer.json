{
    "name": "contaoblackforest/contao-custom-search",
    "description": "This Search Module is for Contao open source cms",
    "keywords": [
        "contao",
        "module",
        "search",
        "custom"
    ],
    "type": "contao-module",
    "require": {
        "php": ">=5.4",
        "contao/core": ">=3.2,<4",
        "contao-community-alliance/composer-plugin": "~2.0",
        "contao-community-alliance/dc-general": "2.0.x-dev",
        "symfony/form": "~2.0",
        "symfony/validator": "~2.0",
        "menatwork/contao-multicolumnwizard": "~3.0"
    },
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Sven Baumann",
            "email": "baumann.sv@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Dominik Tomasi",
            "email": "dominik.tomasi@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/ContaoBlackForest/contao-custom-search/issues",
        "source": "https://github.com/ContaoBlackForest/contao-custom-search"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.3",
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-0": {
            "ContaoBlackForest\\Module\\CustomSearch\\": "src"
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "contao": "system/modules/custom-search"
            }
        },
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
